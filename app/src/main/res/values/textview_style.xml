    <style name="inputbox_parent" parent="android:Widget.EditText">
        <item name="android:layout_height">wrap_content</item>
        <item name="minHeight">44dp</item>d
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">left|center</item>
    </style>

    <style name="inputbox_type_one" parent="inputbox_parent">
        <!--        <item name="android:background">@drawable/common_inputbox_bg_selector</item>-->
        <item name="android:enabled">false</item>

        <item name="android:textSize">@dimen/font_size_14</item>
        <item name="android:paddingLeft">@dimen/spacing_12</item>
    </style>

    <style name="inputbox_spinner" parent="inputbox_parent">
        <item name="android:background">@drawable/common_inputbox_bg_selector</item>
        <item name="android:enabled">false</item>
    </style>

