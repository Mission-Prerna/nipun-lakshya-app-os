<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="label_parent" parent="android:Widget.TextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:maxLines">4</item>
        <item name="android:ellipsize">end</item>
        <item name="android:lineSpacingMultiplier">1</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textIsSelectable">false</item>

    </style>


    <!-- H1 : 24, Medium START-->
    <style name="label_h1_bold" parent="label_parent">
        <item name="android:textColor">@color/color_014D3F</item>
        <item name="android:textSize">@dimen/font_size_24</item>
        <item name="android:textStyle">bold</item>
        <item name="android:lineSpacingExtra">10sp</item>

        <!--        <item name="android:fontFamily">@font/lato_bold</item>-->
    </style>

    <style name="label_h1_normal" parent="label_parent">
        <item name="android:textSize">@dimen/font_size_24</item>
        <item name="android:textColor">@color/color_014D3F</item>
        <!--        <item name="android:fontFamily">@font/lato_regular</item>-->
    </style>

    <style name="label_h1_normal_light" parent="label_parent">
        <item name="android:textSize">@dimen/font_size_24</item>
        <!--        <item name="android:fontFamily">@font/lato_regular</item>-->
    </style>

    <!-- H1 : 24, Medium END-->

    <!-- H2 : 20, bold START-->
    <style name="label_h2_bold" parent="label_parent">
        <item name="android:textColor">@color/color_014D3F</item>
        <item name="android:textSize">@dimen/font_size_20</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="label_h2_normal" parent="label_parent">
        <item name="android:textSize">@dimen/font_size_20</item>
        <item name="android:textColor">@color/color_014D3F</item>
    </style>

    <style name="label_h2_normal_light" parent="label_parent">
        <item name="android:textSize">@dimen/font_size_20</item>
    </style>

    <style name="label_h3_bold" parent="label_parent">
        <item name="android:textColor">@color/color_014D3F</item>
        <item name="android:textSize">@dimen/font_size_16</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="label_h3_normal" parent="label_parent">
        <item name="android:textSize">@dimen/font_size_16</item>
        <item name="android:textColor">@color/color_014D3F</item>
    </style>


    <style name="label_h3_normal_light" parent="label_parent">
        <item name="android:textSize">@dimen/font_size_16</item>
    </style>


    <style name="label_h4_bold" parent="label_parent">
        <item name="android:textColor">@color/color_014D3F</item>
        <item name="android:textSize">@dimen/font_size_14</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="label_h4_normal" parent="label_parent">
        <item name="android:textSize">@dimen/font_size_14</item>
        <item name="android:textColor">@color/color_014D3F</item>
